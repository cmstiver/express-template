{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": "warn",
    "no-underscore-dangle": "off",
    "no-shadow": "off",
    "consistent-return": "off",
    "no-param-reassign": "off",
    "func-names": "off"
  }
}
